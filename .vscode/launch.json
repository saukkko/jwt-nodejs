{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/src/index.ts",
      "outFiles": ["${workspaceFolder}/lib/**/*.js"]
    }
  ]
}
